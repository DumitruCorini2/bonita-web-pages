{
  "designerVersion" : "1.5.10",
  "id" : "customResetButton",
  "name" : "ResetButton",
  "lastUpdate" : 1568361212941,
  "template" : "<div class=\"text-{{ properties.alignment }}\">\r\n    <button\r\n        ng-class=\"'btn btn-' + properties.buttonStyle\"\r\n        ng-click=\"ctrl.action()\"\r\n        type=\"reset\"\r\n        ng-disabled=\"properties.disabled\" ng-bind-html=\"properties.label | uiTranslate\"></button>\r\n</div>",
  "controller" : "",
  "description" : "This is the custom reset button widget.",
  "custom" : true,
  "properties" : [ {
    "label" : "Disabled",
    "name" : "disabled",
    "type" : "boolean",
    "defaultValue" : false,
    "bond" : "expression"
  }, {
    "label" : "Label",
    "name" : "label",
    "type" : "text",
    "defaultValue" : "Reset",
    "bond" : "interpolation"
  }, {
    "label" : "Alignment",
    "name" : "alignment",
    "type" : "choice",
    "defaultValue" : "left",
    "choiceValues" : [ "left", "center", "right" ],
    "bond" : "constant"
  }, {
    "label" : "Style",
    "name" : "buttonStyle",
    "type" : "choice",
    "defaultValue" : "default",
    "choiceValues" : [ "default", "primary", "success", "info", "warning", "danger", "link" ],
    "bond" : "constant"
  } ],
  "assets" : [ ],
  "requiredModules" : [ "ui.bootstrap" ],
  "authRules" : [ "GET|living/application", "GET|living/application-menu", "GET|living/application-page" ],
  "type" : "widget",
  "hasHelp" : false
}